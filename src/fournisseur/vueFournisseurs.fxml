<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="518.0" prefWidth="707.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fournisseur.VueFournisseursController">
   <children>
      <ImageView fitHeight="528.0" fitWidth="955.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../ressources/bg3.jpg" />
         </image>
      </ImageView>
      <Label layoutX="292.0" layoutY="20.0" text="MES FOURNISSEURS" />
      <TableView fx:id="tbl_fournisseurs" layoutX="33.0" layoutY="195.0" onMouseClicked="#tableFournisseurClicked" prefHeight="317.0" prefWidth="654.0">
        <columns>
          <TableColumn fx:id="cln_lib_fournisseurs" prefWidth="193.0" text="Nom Fournisseur">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../ressources/Resources_icones/blog_post.png" />
                     </image>
                  </ImageView>
               </graphic></TableColumn>
          <TableColumn fx:id="cln_telphone" prefWidth="133.0" text="Tel">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../ressources/boutique_ico/tel.png" />
                     </image>
                  </ImageView>
               </graphic></TableColumn>
            <TableColumn fx:id="cln_ifu" prefWidth="48.0" text="IFU">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../ressources/Resources_icones/msoffice_Footer.png" />
                     </image>
                  </ImageView>
               </graphic></TableColumn>
            <TableColumn fx:id="cln_rcm" prefWidth="75.0" text="RCM">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../ressources/Resources_icones/msoffice_Footer1.png" />
                     </image>
                  </ImageView>
               </graphic></TableColumn>
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btn_modifier" layoutX="424.0" layoutY="160.0" mnemonicParsing="false" onMouseClicked="#modifierFournisseurClicked" prefHeight="8.0" prefWidth="95.0" text="Modifier" wrapText="true">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../ressources/boutique_ico/edit.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <HBox layoutX="101.0" layoutY="122.0" prefHeight="32.0" prefWidth="523.0" spacing="7.0">
         <children>
            <TextField fx:id="txt_lib_fournisseur" prefHeight="25.0" prefWidth="112.0" />
            <TextField fx:id="txt_telephone" prefHeight="25.0" prefWidth="132.0" />
            <TextField fx:id="txt_ifu" prefHeight="25.0" prefWidth="118.0" />
            <TextField fx:id="txt_rcm" prefHeight="25.0" prefWidth="112.0" />
         </children>
      </HBox>
      <TextArea fx:id="txt_details" layoutX="450.0" layoutY="60.0" prefHeight="32.0" prefWidth="156.0" />
      <HBox layoutX="101.0" layoutY="106.0" prefHeight="16.0" prefWidth="499.0">
         <children>
            <Label prefHeight="15.0" prefWidth="129.0" text="Nom Fournisseur">
               <font>
                  <Font name="DejaVu Sans Condensed Bold" size="13.0" />
               </font>
            </Label>
            <Label prefHeight="15.0" prefWidth="131.0" text="Telephone" />
            <Label prefHeight="15.0" prefWidth="130.0" text="IFU" />
            <Label text="RCM" />
         </children>
      </HBox>
      <Label layoutX="401.0" layoutY="67.0" text="Details" />
      <Button fx:id="btnNouveau" layoutX="199.0" layoutY="160.0" mnemonicParsing="false" onMouseClicked="#clickBtnNouveau" text="Nouveau" wrapText="true">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../ressources/boutique_ico/new.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="btn_ajouter" layoutX="316.0" layoutY="161.0" mnemonicParsing="false" onMouseClicked="#clickBtnAjouter" prefHeight="15.0" prefWidth="93.0" text="Ajouter">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../ressources/boutique_ico/plus.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Label layoutX="125.0" layoutY="66.0" text="Recherche" textFill="#5f0de4">
         <font>
            <Font name="DejaVu Sans Condensed Bold" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="txt_recherche" layoutX="194.0" layoutY="63.0" onKeyReleased="#rechercheFournisseur" prefHeight="25.0" prefWidth="113.0" />
   </children>
</AnchorPane>
